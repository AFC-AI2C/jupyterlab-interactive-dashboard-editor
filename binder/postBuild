#!/bin/bash

# install extension
jupyter labextension install @jupyter-widgets/jupyterlab-manager --no-build
jupyter labextension install jupyterlab-interactive-dashboard-editor

# remove unnecessary directories when using the binder
rm -rf src
rm -rf style
rm -rf Design
rm -rf .github
rm -rf binder
rm -rf docs
rm -rf demo
rm package.json
rm tsconfig.json
rm yarn.lock
rm MANIFEST.in
rm covid-19.ipynb
rm sample.dash

jupyter lab clean
